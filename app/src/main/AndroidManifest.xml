        </activity>
        <activity android:name=".HowToListActivity" />
        <activity android:name=".HowToDetailsActivity"></activity>

        <meta-data
            android:name="com.google.android.gms.ads.APPLICATION_ID"
            android:value="ca-app-pub-6081186777692750~9784125877" />

    </application>

</manifest>